<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ListView fx:id="rentsListView" layoutX="12.0" layoutY="14.0" prefHeight="200.0" prefWidth="577.0" />
      <GridPane layoutX="12.0" layoutY="221.0" prefHeight="130.0" prefWidth="577.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="284.0" minWidth="10.0" prefWidth="93.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="484.0" minWidth="10.0" prefWidth="484.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Id" />
            <Label text="Car id" GridPane.rowIndex="1" />
            <Label text="Start date" GridPane.rowIndex="2" />
            <Label text="End date" GridPane.rowIndex="3" />
            <TextField fx:id="idTextField" GridPane.columnIndex="1" />
            <DatePicker fx:id="startDateDatePicker" prefHeight="31.0" prefWidth="502.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <DatePicker fx:id="endDateDatePicker" prefHeight="32.0" prefWidth="501.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <ComboBox fx:id="carIdComboBox" prefHeight="31.0" prefWidth="485.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="addRentButton" layoutX="12.0" layoutY="355.0" mnemonicParsing="false" onMouseClicked="#onAddRentBtnClicked" text="Add" />
   </children>
</AnchorPane>
